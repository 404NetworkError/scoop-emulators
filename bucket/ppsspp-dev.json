        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.13.2-1160-gb56bd0d0f&platform=windows-amd64#/dl.7z",
            "hash": "4be35a6af1d151ada643185e8fec6af10099848cd0e6f29c9d51319da330a294",
