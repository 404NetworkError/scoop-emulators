            ],
            "shortcuts": [
                [
                    "PPSSPPWindows.exe",
                    "PPSSPP (development)"
                ]
            ]
        },
        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.13.2-1050-g3e0b3ba1a&platform=windows-amd64#/dl.7z",
            "hash": "f88a128f724432238fd01026e830c5a38ed41b3cbc3bbaac4a5d58371e69e992",
            "bin": [
                [
                    "PPSSPPWindows64.exe",
                    "ppsspp-dev"
