        }
    },
    "bin": [
        [
            "openmsx.exe",
            "openmex-dev"
        ]
    ],
    "shortcuts": [
        [
            "openmsx.exe",
            "openMSX\\openMSX (Development)"
        ],
        [
            "Catapult\\bin\\Catapult.exe",
            "openMSX\\Catapult (Development)"
        ]
    ],
    "checkver": {
