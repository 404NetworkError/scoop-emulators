            ],
            "shortcuts": [
                [
                    "PPSSPPWindows.exe",
                    "PPSSPP (dev)"
                ]
            ]
        },
        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.14.4-250-gc75a7e0f5&platform=windows-amd64#/dl.7z",
            "hash": "c1a410a5f6be6a7ba20d767dbaf2991de97ce5769d31ee3b4d6e017e33747a2d",
            "bin": [
                [
                    "PPSSPPWindows64.exe",
                    "ppsspp-dev"
                ]
            ],
