        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.14.1-67-g47e85cd2b&platform=windows-amd64#/dl.7z",
            "hash": "45d15ef137cd8d02c61d50b1ca7404606d1f561542d774684b3ed2607159e659",
