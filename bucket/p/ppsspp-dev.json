        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.14.1-72-g545c2a0e2&platform=windows-amd64#/dl.7z",
            "hash": "0dd52f9dfae6ea3bb319b0df9e791bb82451fb19c4f9ade6324ca03df33742e2",
