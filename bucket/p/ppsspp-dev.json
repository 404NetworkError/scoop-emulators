        },
        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.14.4-541-gea9245d90&platform=windows-amd64#/dl.7z",
            "hash": "351666c7a2b53c25095bf643ff3beb4d8d070fef52ccc3249061222e8e6462c7",
            "bin": [
