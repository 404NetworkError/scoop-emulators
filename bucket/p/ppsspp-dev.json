        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.14.4-142-ge2a9abaf5&platform=windows-amd64#/dl.7z",
            "hash": "d61ea828f4c68ae655cee786ca0533cfba41288e05f7e967ea3aed18e50a8231",
