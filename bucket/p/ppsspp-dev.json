            "hash": "b5e5579dd04af900a8ec292e9592c174d31c95ab6e5271759e59cdee4604cbfe",
            "bin": [
                [
                    "PPSSPPWindows.exe",
                    "ppsspp-dev"
                ]
            ],
            "shortcuts": [
                [
                    "PPSSPPWindows.exe",
                    "PPSSPP (dev)"
                ]
            ]
        },
        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.13.2-2051-g9eb97830c&platform=windows-amd64#/dl.7z",
            "hash": "7c3fe94fb1e1ec25cc4af506c7e554fd15f722de92fe3fe7690244eab2df3746",
            "bin": [
                [
                    "PPSSPPWindows64.exe",
                    "ppsspp-dev"
                ]
            ],
            "shortcuts": [
                [
                    "PPSSPPWindows64.exe",
                    "PPSSPP (dev)"
                ]
            ]
        }
    },
    "extract_dir": "ppsspp",
    "pre_install": [
        "if (!(Test-Path \"$persist_dir\")) {",
        "   New-Item \"$persist_dir\" -ItemType Directory | Out-Null",
        "   New-Item \"$persist_dir\\memstick\" -ItemType Directory | Out-Null",
        "}"
    ],
    "persist": [
        "memstick",
        "assets",
        "demos"
    ],
    "checkver": {
        "url": "https://buildbot.orphis.net/ppsspp/index.php",
        "regex": "=v(?<version>[\\d.]+-[\\d]+)-g(?<commit>[a-f\\d]+)&[\\w;=]+-amd64"
    },
    "autoupdate": {
        "architecture": {
            "32bit": {
                "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v$version-g$matchCommit&platform=windows-x86#/dl.7z"
            },
            "64bit": {
                "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v$version-g$matchCommit&platform=windows-amd64#/dl.7z"
            }
        }
    }
}
