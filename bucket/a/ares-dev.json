        "regex": "^(?<version>[^ ]+) (?<year>\\d{4})-(?<month>\\d{2})-(?<day>\\d{2})[^ ]+ (?<hash>.{7})(?<rest>.*)$",
        "replace": "${version}-${year}${month}${day}-${hash}"
    },
    "autoupdate": {
        "url": "https://nightly.link/ares-emulator/ares/workflows/build/master/ares-windows.zip"
    }
